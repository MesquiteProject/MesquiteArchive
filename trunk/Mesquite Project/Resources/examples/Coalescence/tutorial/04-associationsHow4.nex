BEGIN TAXA;
	TITLE Species;
	DIMENSIONS NTAX=3;
	TAXLABELS
		speciesA speciesB speciesC 
	;

END;



BEGIN TAXA;
	TITLE Genes;
	DIMENSIONS NTAX=5;
	TAXLABELS
		geneA1 geneA2 geneB geneC1 geneC2 
	;

END;


BEGIN TaxaAssociation;
TITLE how_genes_fit_into_species;
TAXA Species ,  Genes;
ASSOCIATES 

	speciesA /  geneA1 geneA2 , 
	speciesB /  geneB , 
	speciesC /  geneC1 geneC2

;

END;


BEGIN TREES;
	Title gene_trees;
	LINK Taxa = Genes;
	TRANSLATE
		1 geneA1,
		2 geneA2,
		3 geneB,
		4 geneC1,
		5 geneC2;
	TREE gene_tree = ((1,2),(3,(4,5)));

END;


BEGIN TREES;
	Title species_trees;
	LINK Taxa = Species;
	TRANSLATE
		1 speciesA,
		2 speciesB,
		3 speciesC;
	TREE species_tree = ((1,2),3);

END;


