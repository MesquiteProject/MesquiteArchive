	speciesA /  g0 g1 g2 g3 , 
	speciesB /  g4 g5 g6 , 
	speciesC /  g7 g8 g9 , 
	speciesD /  g10 g11 g12 , 
	speciesE /  g13 g14 g15

;

END;


