BEGIN TAXA;
	TITLE Boxes;
	DIMENSIONS NTAX=4;
	TAXLABELS
		Box_1 Box_2 Box_3 Box_4 
	;

END;


BEGIN CHARACTERS;
	TITLE  Landmark_positions;
	DIMENSIONS NCHAR=4;
	FORMAT DATATYPE = CONTINUOUS ITEMS = (x y ) ;
	MATRIX
	Box_1   (? ?) (? ?) (? ?) (? ?)
	Box_2   (? ?) (? ?) (? ?) (? ?)
	Box_3   (? ?) (? ?) (? ?) (? ?)
	Box_4   (? ?) (? ?) (? ?) (? ?)

;

END;

BEGIN ASSUMPTIONS;
	TYPESET * UNTITLED   =  Squared:  1 -  4;

END;

BEGIN MESQUITECHARMODELS;
